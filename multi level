 class A 
{
    void showA()
    {
        System.out.println(" base class");
    }
}
class B extends A
{
    void showB()
    {
        System.out.println(" sub class");
    }
}
class C extends B 
{
    void showC()
    {
        System.out.println(" super sub class");
    }
} 
public class Demo
{
public static void main(String[] args)
{
    A obj = new A();
    B obj2 = new B();
    obj.showA();
    obj2.showA();
     obj2.showB();
}
}
